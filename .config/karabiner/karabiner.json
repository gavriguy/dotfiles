{
  "global": {
    "show_in_menu_bar": false
  },
  "profiles": [
    {
      "name": "Default",
      "complex_modifications": {
        "rules": [
          {
            "description": "Hyper Key (⌃⌥⇧⌘)",
            "manipulators": [
              {
                "description": "Right Command -> Hyper Key",
                "from": {
                  "key_code": "right_command"
                },
                "to": [
                  {
                    "key_code": "left_shift",
                    "modifiers": [
                      "left_command",
                      "left_control",
                      "left_option"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Caps Lock settings",
            "manipulators": [
              {
                "description": "Caps Lock -> Ctrl",
                "from": {
                  "key_code": "caps_lock"
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "escape"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Hyper Key sublayer \"s\"",
            "manipulators": [
              {
                "description": "Toggle Hyper sublayer s",
                "type": "basic",
                "from": {
                  "key_code": "s",
                  "modifiers": {
                    "mandatory": [
                      "left_command",
                      "left_control",
                      "left_shift",
                      "left_option"
                    ]
                  }
                },
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_s",
                      "value": 0
                    }
                  }
                ],
                "to": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_s",
                      "value": 1
                    }
                  }
                ],
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_equal_sign",
                    "value": 0
                  }
                ]
              },
              {
                "to": [
                  {
                    "shell_command": "/usr/local/bin/yabai --start-service"
                  }
                ],
                "type": "basic",
                "from": {
                  "key_code": "h",
                  "modifiers": {
                    "mandatory": [
                      "any"
                    ]
                  }
                },
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_s",
                    "value": 1
                  }
                ]
              },
              {
                "to": [
                  {
                    "shell_command": "/usr/local/bin/yabai --stop-service"
                  }
                ],
                "type": "basic",
                "from": {
                  "key_code": "j",
                  "modifiers": {
                    "mandatory": [
                      "any"
                    ]
                  }
                },
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_s",
                    "value": 1
                  }
                ]
              }
            ]
          },
          {
            "description": "Hyper Key sublayer \"equal_sign\"",
            "manipulators": [
              {
                "description": "Toggle Hyper sublayer equal_sign",
                "type": "basic",
                "from": {
                  "key_code": "equal_sign",
                  "modifiers": {
                    "mandatory": [
                      "left_command",
                      "left_control",
                      "left_shift",
                      "left_option"
                    ]
                  }
                },
                "to_after_key_up": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_equal_sign",
                      "value": 0
                    }
                  }
                ],
                "to": [
                  {
                    "set_variable": {
                      "name": "hyper_sublayer_equal_sign",
                      "value": 1
                    }
                  }
                ],
                "conditions": [
                  {
                    "type": "variable_if",
                    "name": "hyper_sublayer_s",
                    "value": 0
                  }
                ]
              },
              {
                "to": [
                  {
                    "shell_command": "open -a 'WezTerm.app'"
                  }
                ],
